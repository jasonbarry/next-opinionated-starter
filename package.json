{
  "name": "dashboard",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "husky": {
    "hooks": {
      "post-merge": "yarn",
      "pre-commit": "lint-staged",
      "pre-push": "yarn test:full"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn pretty",
      "git add"
    ],
    "yarn.lock": [
      "yarn libdefs",
      "git add"
    ]
  },
  "scripts": {
    "analyze": "BUNDLE_ANALYZE=browser yarn build",
    "build": "NODE_ENV=production next build",
    "dev": "NODE_ENV=development node server.js",
    "libdefs": "flow-typed install --overwrite",
    "lint": "eslint '**/*.{js,jsx}' --cache --ignore-path .prettierignore",
    "pretty": "prettier --write '**/*.{js,jsx}'",
    "serve": "NODE_ENV=production node server.js",
    "test": "NODE_ENV=test jest",
    "test:full": "yarn flow && yarn lint"
  },
  "dependencies": {
    "@babel/plugin-transform-flow-strip-types": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "babel-plugin-module-resolver": "^3.1.3",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "next": "^7.0.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-import-resolver-babel-module": "^5.0.0",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.91.0",
    "flow-typed": "^2.5.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.1"
  }
}
